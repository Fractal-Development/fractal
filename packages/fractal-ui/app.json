{
    "expo": {
        "entryPoint": "./App.tsx",
        "name": "Fractal-UI",
        "slug": "Fractal-UI",
        "version": "1.0.0",
        "orientation": "default",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "fractal.fractalui",
            "userInterfaceStyle": "automatic"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "android": {
            "softwareKeyboardLayoutMode": "pan",
            "package": "fractal.fractalui"
        }
    }
}
